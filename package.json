{
  "name": "unevent-website",
  "version": "1.0.0",
  "description": "UNevent - Событие как искусство. Профессиональная организация мероприятий полного цикла.",
  "main": "public/index.html",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "serve": "vite preview --port 3000",
    "lint": "eslint src/js/**/*.js",
    "lint:fix": "eslint src/js/**/*.js --fix",
    "format": "prettier --write src/**/*.{js,css,html}",
    "optimize:images": "imagemin src/images/*.{jpg,png,svg} --out-dir=public/images --plugin=imagemin-mozjpeg --plugin=imagemin-pngquant --plugin=imagemin-svgo"
  },
  "keywords": [
    "event",
    "agency",
    "st-petersburg",
    "corporate-events",
    "entertainment"
  ],
  "author": "UNevent Team",
  "license": "MIT",
  "devDependencies": {
    "vite": "^5.0.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "imagemin": "^8.0.0",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^9.0.0",
    "imagemin-svgo": "^10.0.0"
  },
  "dependencies": {
    "lazysizes": "^5.3.0"
  }
}
